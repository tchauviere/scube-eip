{% extends 'ScubeBaseBundle:Base:core_application.html.twig' %}


{% block body %}
{{ parent() }}

<div class="Application">

<table class="table_content">
<tr>
	<td class="sidebar_left">
		{% include 'ScubeAdminAppearanceBundle:AdminAppearance:menu.html.twig' %}
	</td>
	<td class="sidebar_main">
		<h1>General appearance</h1>

		<form action="{{ path('ScubeAdminAppearanceBundle_general') }}" method="post" {{ form_enctype(form) }}>
			{{ form_widget(form) }}
		
			<input type="submit" value="Save" />
		</form>
	</td>
</tr>
</table>

</div>

{% endblock %}

