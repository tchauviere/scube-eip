{% extends 'ScubeBaseBundle:Base:core_application.html.twig' %}


{% block body %}
{{ parent() }}

<div class="Application">

<table class="table_content">
<tr>
	<td class="sidebar_left">
		<ul class="sidebar_navigation">
			<li><a href="{{ path("AdminSettingsBundle_homepage")}}">Manage settings</a></li>
		</ul>
	</td>
	<td class="sidebar_main">
		<h1>Edit setting  {{setting.getKey()}} </h1>
	
		{% if success %}
			<p class="form_success">The setting has been successfully updated.</p>
		{% endif %}
		<form action="{{ path("AdminSettingsBundle_edit", { 'id': setting.getId()})}}" method="post" {{ form_enctype(form) }}>
			{{ form_widget(form) }}
		
			<input type="submit" value="Save" />
		</form>
	</td>
</tr>
</div>

{% endblock %}
